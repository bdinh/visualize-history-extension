{
  "short_name": "History Visualized",
  "name": "History Visualized",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Visualize History"
  },
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "storage",
    "history",
    "topSites",
    "tabs",
    "chrome://favicon/"
  ],
  "content_security_policy": "img-src chrome://favicon;"
}
